{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "cd ../frontend && yarn start",
    "server": "node ./bin/www",
    "start": "cross-env NODE_ENV=development concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "prueba": {
    "server": "nodemon --exec babel-node server.js",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  }
}
